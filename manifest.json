{
    "manifest_version": 3,
    "name": "Cookie Manager Extension",
    "version": "1.0",
    "description": "Enhance user control and transparency in browser tracking and cookie consent.",
    "permissions": ["cookies", "activeTab", "storage", "scripting"],
    "host_permissions": ["<all_urls>"],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup/popup.html",
      "default_icon": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
      }
    },
    "options_page": "options/options.html",
    "icons": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  }
  